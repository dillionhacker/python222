#include <appinfo.rh>
#include <python.rsg>

UID2 KUidAppRegistrationResourceFile
UID3 0x10201510 // Python app UID, app specific

RESOURCE APP_REGISTRATION_INFO
    {
    app_file = "PYTHON";
    attributes = KAppBuiltAsDll;
    localisable_resource_file = "\\System\\Apps\\Python\\Python";
    localisable_resource_id = R_PYTHON_LOCALISABLE_APP_INFO;
    }
